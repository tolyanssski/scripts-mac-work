#!/usr/bin/env bash

PAYLOAD='{"email":"tolyanski222@mail.com.kg","password":"Termit123!","refId":"ref-123","utmSource":"campaign","utmMedium":"cpc","utmCampaign":"spring-launch","utmTerm":"auth","utmContent":"banner"}'

curl -i \
  -X POST \
  -H "Accept: application/json" \
  -H "Content-Type: application/json" \
  -d "${PAYLOAD}" \
  "https://api.singapore.glexdev.com/auth/v1/log-in"

printf '\n\n'
